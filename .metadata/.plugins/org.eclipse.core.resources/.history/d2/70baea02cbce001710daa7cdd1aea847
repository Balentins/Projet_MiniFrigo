package Model;

public class Model {
	
	public static boolean cond;
	String valeur;


	public String getValeur() {
		return valeur;
	}

	public void setValeur(String valeur) 
	{
		this.valeur = valeur;
		System.out.println(valeur);
	}
	public boolean Condensation(boolean Cond)
	{
		int ACond=10;
		if (ACond <= 25)
		{
			Cond = false;
		}
		else 
		{
			Cond=true;
		}
	return (boolean)(Cond);
	}

}
